{
    "name": "reactinmodal",
    "version": "1.0.9",
    "description": "React In Modal. Accessibility friendly with tab trap accepts action to close the modal along with custom styling",
    "repository": {
        "type": "git",
        "url": "https://github.com/vageez/react-in-modal.git"
    },
    "author": "Angelo Vagenas",
    "homepage": "https://github.com/vageez/react-in-modal",
    "license": "MIT",
    "keywords": [
        "react-component",
        "react",
        "react-createportal",
        "accessibility"
    ],
    "options": {
        "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
    },
    "scripts": {
        "build": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
        "lint": "eslint ./src",
        "test": "mocha $npm_package_options_mocha"
    },
    "devDependencies": {
        "babel-cli": "^6.6.4",
        "babel-core": "^6.7.4",
        "babel-eslint": "^6.0.2",
        "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
        "babel-polyfill": "^6.7.4",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-2": "^6.5.0",
        "chai": "^3.5.0",
        "enzyme": "^2.2.0",
        "eslint": "^4.18.2",
        "eslint-plugin-babel": "^3.1.0",
        "eslint-plugin-react": "^4.2.3",
        "nodemon": "^1.9.1",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "sinon": "^1.17.3"
    },
    "peerDependencies": {
        "react": "^16.7.0",
        "react-dom": "^16.7.0"
    },
    "dependencies": {
        "babel-runtime": "^6.6.1",
        "prop-types": "^15.7.2"
    }
}
